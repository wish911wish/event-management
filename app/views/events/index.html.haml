= render "./shared/header"

%body.main
  .container
    .row
      - @events.each do |event|
        .col-md-6
          = link_to event_path(event) do
            .card.mx-auto.mb-2.border-0.event
              =render "event", event: event
    .row
      .col-12.text-center
        = paginate @events

= render "./shared/footer"
